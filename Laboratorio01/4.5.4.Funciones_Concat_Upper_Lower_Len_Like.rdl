  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HumanResources</DataSourceName>
        <CommandText>select CONCAT(UPPER(SUBSTRING(last_name,1,1)), LOWER(SUBSTRING (last_name,2,LEN(last_name)-1))) AS Apellido, len(last_name) as Longitud
from employees
where last_name like 'J%' or last_name like 'A%' or last_name like 'M%'
order by last_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
