      <Query>
        <DataSourceName>HumanResources</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT CAST(employee_id as varchar) + ' , ' + first_name + ' , ' + last_name + ' , ' + email + ' , ' + 
phone_number + ' , ' + CAST(hire_date as varchar) + ' , ' + job_id + ' , ' +
CAST(salary as varchar) + ',' +CAST(manager_id as varchar) + ' , ' + CAST(department_id as varchar)
as "Los Empleados"
FROM EMPLOYEES</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
