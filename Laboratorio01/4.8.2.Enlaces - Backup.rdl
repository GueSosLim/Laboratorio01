      </Fields>
      <Query>
        <DataSourceName>HumanResources</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT	emp.last_name AS Apellidos,
		emp.department_id AS 'N° de Departamento',
		dep.department_name AS 'Departamento'
FROM employees AS emp
INNER JOIN departments AS dep ON dep.department_id = emp.department_id;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
