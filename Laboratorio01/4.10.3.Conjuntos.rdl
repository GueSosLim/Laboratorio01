  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HumanResources</DataSourceName>
        <CommandText>SELECT	job.job_id AS 'Cod Puesto',
			dep.department_id AS 'Cod Departamento'
	FROM jobs AS job
	INNER JOIN employees AS emp ON emp.job_id = job.job_id
	INNER JOIN departments AS dep ON dep.department_id = emp.department_id
	WHERE dep.department_id = 10

	UNION

	SELECT	job0.job_id AS 'Cod Puesto',
			dep0.department_id AS 'Cod Departamento'
	FROM jobs AS job0
	INNER JOIN employees AS emp0 ON emp0.job_id = job0.job_id
	INNER JOIN departments AS dep0 ON dep0.department_id = emp0.department_id
	WHERE dep0.department_id = 50

	UNION

	SELECT	job1.job_id AS 'Cod Puesto',
			dep1.department_id AS 'Cod Departamento'
	FROM jobs AS job1
	INNER JOIN employees AS emp1 ON emp1.job_id = job1.job_id
	INNER JOIN departments AS dep1 ON dep1.department_id = emp1.department_id
	WHERE dep1.department_id = 20</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
