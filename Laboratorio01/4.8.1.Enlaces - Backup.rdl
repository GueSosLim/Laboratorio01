      </Fields>
      <Query>
        <DataSourceName>HumanResources</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT	loc.location_id AS Ubicación,
		loc.street_adress AS Dirección,
		loc.city AS Ciudad,
		loc.state_province AS 'Estado o Provincia',
		cou.country_name AS País
FROM locations AS loc
INNER JOIN countries AS cou ON cou.country_id = loc.country_id;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
